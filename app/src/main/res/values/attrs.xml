<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 圆形进度条 -->
    <declare-styleable name="ArcProgressView">
        <attr name="antiAlias" format="boolean"/>
        <attr name="startAngle" />
        <attr name="sweepAngle" />
        <attr name="animTime" />
        <attr name="maxValue" />
        <attr name="value" />
        <attr name="precision" />
        <attr name="valueSize" />
        <attr name="valueColor" />
        <attr name="textOffsetPercentInRadius" />
        <!-- 绘制内容相应的提示语 -->
        <attr name="hint" format="string"/>
        <attr name="hintSize" />
        <attr name="hintColor" />
        <!-- 绘制内容的单位 -->
        <attr name="unit" />
        <attr name="unitSize" />
        <attr name="unitColor" />
        <!-- 圆弧宽度 -->
        <attr name="mStrokeWith" format="dimension" />
        <!--<attr name="arcWidth" />-->
        <attr name="arcColors" />
        <!-- 背景圆弧颜色 -->
        <attr name="bgArcColor" />
        <!-- 背景圆弧宽度 -->
        <!--<attr name="bgArcWidth" format="dimension" />-->
    </declare-styleable>
</resources>